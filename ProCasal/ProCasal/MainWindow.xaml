        <TextBox HorizontalAlignment="Left" Height="27" Margin="162,122,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="147"/>
        <Button Content="Cadastrar Pessoa" HorizontalAlignment="Left" Margin="146,320,0,0" VerticalAlignment="Top" Width="157" Height="49" RenderTransformOrigin="0.706,0.579" FontSize="16" FontWeight="Bold" FontStyle="Italic"/>
