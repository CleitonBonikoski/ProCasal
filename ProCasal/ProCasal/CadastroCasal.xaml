        <TextBlock Foreground="Blue" HorizontalAlignment="Left" Margin="332,134,0,0" TextWrapping="Wrap" Text="Detalhamento" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" FontStyle="Italic"/>

